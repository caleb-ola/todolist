<%- include("partials/header"); -%>

    <div class="box" id="heading">
        <h1 ><%= TDdate %></h1>
    </div>
    
    <div class="box">
       
        <form action="/delete" method="POST">
            <% latest.forEach( (later,i) => { %> 
            <div class ="item">
            <input type="checkbox" name="summi" value="<%= later._id %>" onChange="this.form.submit()">
            <p><%= later.name %></p>
            </div>
            <% }) %>
            <input type="hidden" name="lastDel" value="<%= TDdate %>">
        </form>
        
    
    <form action="/" method="post">
        <input type="text" name="newList" placeholder="Add Item" autocomplete="off"/>
        <button type="submit" name="submit" value="<%= TDdate %>">+</button>
    </form>
    
    </div>
   
 <%- include("partials/footer"); -%>